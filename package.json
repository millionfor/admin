    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "ghooks": "^2.0.4",
    "globby": "^8.0.1",
    "html-loader": "^0.5.5",
    "html-webpack-ext-plugin": "^1.0.0",
    "html-webpack-plugin": "^3.2.0",
    "moment": "^2.22.2",
    "node-sass": "^4.9.2",
    "optimize-css-assets-webpack-plugin": "3.2.0",
    "postcss-loader": "^2.1.6",
    "qn": "^1.3.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.1.0",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "url-loader": "^1.0.1",
