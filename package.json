{
  "name": "pc",
  "version": "1.0.0",
  "description": "pc project",
  "scripts": {
    "build": "cross-env NODE_ENV=production npm run lint:fix && npm run clean && webpack --config ./build/webpack.config.prod.js",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config ./build/webpack.config.dev.js",
