    "url-loader": "^1.0.1",
    "vue-loader": "^13.7.0",
    "vue-style-loader": "^4.1.1",
    "vue-template-compiler": "^2.5.16",
    "vuex-router-sync": "^4.1.2",
    "webpack": "^3.12.0",
    "webpack-dev-server": "^2.11.2",
    "webpack-merge": "^4.1.4"
