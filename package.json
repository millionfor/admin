    "vue-template-compiler": "^2.5.16",
    "vuex-router-sync": "^4.1.2",
    "webpack": "^3.12.0",
    "webpack-dev-server": "^3.9.0",
    "webpack-merge": "^4.1.4"
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "src/font/*",
      "dist/**",
      "test/coverage/**",
      "karma.conf.js"
    ]
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run lint && npm run style"
    }
  },
  "devDependencies": {
    "@vue/test-utils": "^1.0.0-beta.24",
    "jasmine-core": "^3.2.1",
