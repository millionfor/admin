  "scripts": {
    "build": "npm run lint && npm run clean && cross-env NODE_ENV=production webpack --config ./build/webpack.config.prod.js",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config ./build/webpack.config.dev.js",
    "clean": "rimraf dist",
    "lint": "standard \"**/*.{js,vue}\"",
    "lint:fix": "standard \"**/*.{js,vue}\" --fix",
    "style": "stylelint \"**/*.{scss,vue}\"",
    "style:fix": "stylelint \"**/*.{scss,vue}\" --fix",
    "start": "npm run dev",
    "test": "npm run lint && karma start test/karma.conf.js"
